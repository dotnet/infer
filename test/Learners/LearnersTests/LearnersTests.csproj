    <ProjectReference Include="..\..\..\src\Learners\Core\Core.csproj" />
    <ProjectReference Include="..\..\..\src\Learners\Recommender\Recommender.csproj" />
    <ProjectReference Include="..\..\..\src\Learners\Runners\CommandLine\CommandLine.csproj" />
